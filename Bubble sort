#include <iostream>
#include<array>
using namespace std;
void bubblesort(int array[],int n);

int main()
{
    int arr[100];
    int n;
    cin >> n;
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    cout << "Before Sorting" << endl;
    for(int i=0;i<n;i++)
    {
        cout << arr[i] << " ";
    }
    bubblesort(arr,n);
    cout << endl << "After Sorting" << endl;
    for(int i=0;i<n;i++)
    {
        cout << arr[i] << " ";
    }
}
void bubblesort(int array[],int n)
{
    for(int i=0;i<n;i++){
        for(int j=0;j<n-1;j++){
            if(array[j]>array[j+1]){
                int temp=array[j];
                array[j]=array[j+1];
                array[j+1]=temp;
            }
        }
    }
}
